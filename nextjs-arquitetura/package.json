      "packages/*",
      "projects/*",
      "setup/*"
    ]
  },
  "dependencies": {
    "eslint": "8.14.0",
    "prettier": "2.6.2",
    "@types/styled-components": "5.1.34",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/jest": "29.5.12",
    "typescript": "4.5.5"
  },
  "scripts": {
